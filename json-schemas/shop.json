{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "Shop",
    "description": "Location where Items should be purchased",
    "examples": [
        {
            "name": "DM",
            "id": "fa91ff13-2362-413a-9b5b-4e0669b79a00",
            "entry_order": [
                "a15cf024-9cbe-4cfd-8221-a025b3f1d7d7",
                "54caef47-e027-4b75-83db-73069bce9d63"
            ]
        }
    ],
    "required": [
        "name",
        "id",
        "entry_order",
        "1597923253610"
    ],
    "properties": {
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "name",
            "description": "Human-readable name of the shop",
            "examples": [
                "DM"
            ]
        },
        "id": {
            "$id": "#/properties/id",
            "type": "string",
            "title": "identifier",
            "description": "UUID uniquely identifying a shop",
            "examples": [
                "fa91ff13-2362-413a-9b5b-4e0669b79a00"
            ]
        },
        "entry_order": {
            "$id": "#/properties/entry_order",
            "type": "array",
            "title": "entry_order",
            "description": "Partical order of entries, that have been purchased at this shop",
            "examples": [
                [
                    "a15cf024-9cbe-4cfd-8221-a025b3f1d7d7",
                    "54caef47-e027-4b75-83db-73069bce9d63"
                ]
            ],
            "uniqueItems": true,
            "items": {
                "$id": "#/properties/entry_order/items",
                "type": "string",
                "title": "identifier",
                "description": "UUID referencing an entry from the entries collection",
                "examples": [
                    "a15cf024-9cbe-4cfd-8221-a025b3f1d7d7",
                    "54caef47-e027-4b75-83db-73069bce9d63"
                ]
            }
        },
        "last_modified": {
            "$id": "#/properties/last_modified",
            "type": [
                "integer",
                "null"
            ],
            "title": "Last Modified",
            "description": "Last time, the record was modified",
            "examples": [
                1597923253610
            ]
        }
    }
}