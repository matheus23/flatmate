{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "description": "What is currently visible in the Shopping List",
    "examples": [
        {
            "entry": "c260bc24-401c-4562-8acf-4fea396a5812",
            "id": "a0b85d33-a57e-40a3-a5a3-f04d61809f97",
            "checked": false
        }
    ],
    "required": [
        "entry",
        "id",
        "checked"
    ],
    "title": "Shopping List Items",
    "properties": {
        "entry": {
            "description": "The Shopping List Entry that represents this Item",
            "examples": [
                "c260bc24-401c-4562-8acf-4fea396a5812"
            ],
            "title": "Reference to Shopping List Entry",
            "type": "string"
        },
        "id": {
            "description": "Identifier",
            "examples": [
                "a0b85d33-a57e-40a3-a5a3-f04d61809f97"
            ],
            "title": "UUIDv4",
            "type": "string"
        },
        "checked": {
            "default": false,
            "description": "Whether the Item is displayed as checked in the UI.",
            "examples": [
                true,
                false
            ],
            "title": "checked",
            "type": "boolean"
        }
    }
}